{
    "files": [
        "../Api.ts",
        "Misc.ts",
        "Events.ts",
        "Font.ts"
        // NOTE: these are manually included by the DOM engine.
    ],
    "compilerOptions": {
        "allowJs": false,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "noEmitOnError": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitThis": true,
        "outFile": "../../Temp/Scripts/Engine/Shared.js",
        "target": "es3"
    }
}